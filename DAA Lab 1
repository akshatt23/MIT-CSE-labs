#include<stdio.h>
int euclid(int m , int n){
  int opcount = 0;
  while(n!= 0){
    int r=m%n;
    opcount++;
    m=n;
    n=r;
  }
  printf("Opcount = %d\n",opcount);
  return m;
}
int main(){
  int m,n;
  printf("Enter m and n: ");
  scanf("%d %d",&m,&n);
  printf("GCD of %d and %d is %d \n",m,n,euclid(m,n));
  return 0;
}
